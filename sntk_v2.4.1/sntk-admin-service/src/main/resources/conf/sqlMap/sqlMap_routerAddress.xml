<?xml version="1.0" encoding="UTF-8" ?>
<sqlMap namespace="router_address" jdbcTimeout="5">
    <!-- 根据业务类型查询路由地址 -->
    <sql id="queryRouterAddressInfo">
        <![CDATA[
           SELECT
                ROUTER_URL
           FROM
                O2OB_ROUTER_ADDRESS
           WHERE
                URL_STATUS = '1'
           AND
                MODULE_TYPE=:moduleType
        ]]>
    </sql>
</sqlMap>