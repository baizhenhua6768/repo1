<?xml version="1.0" encoding="UTF-8" ?>
<configuration debug="false">
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%-5p %d{HH:mm:ss.SSS} [%t] %c{36}#%M %n -- %m%n</pattern>
        </encoder>
    </appender>
    <logger name="com.suning.sntk" level="DEBUG"/>

    <root level="INFO">
        <appender-ref ref="STDOUT"/>
    </root>
</configuration>