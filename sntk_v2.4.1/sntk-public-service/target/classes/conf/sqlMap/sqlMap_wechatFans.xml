<?xml version="1.0" encoding="UTF-8" ?>
<sqlMap namespace="wechatFans" jdbcTimeout="5">
    <sql id="findByOpenId">
        <![CDATA[
 			SELECT
                id AS id,
                open_id AS openId
            FROM
                mb_wechat_fans
            WHERE
                open_id = :openId
         ]]>
    </sql>
</sqlMap>