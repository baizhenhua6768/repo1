<?xml version="1.0" encoding="UTF-8" ?>
<sqlMap namespace="o2oStore_detail" jdbcTimeout="5">
    <!-- 根据门店编码查询城市编码 -->
    <sql id="queryCityCodeByStoreCode">
        <![CDATA[
            SELECT
                FINANCE_ORG_ID
            FROM
                o2o_store_detail
            WHERE
                store_code =:storeCode
        ]]>
    </sql>
</sqlMap>
